<RecipeDefs>

  <RecipeDef>
    <defName>MakeHeart</defName>
    <label>Make Replacement Heart</label>
    <description>Make an organic replacement heart.</description>
    <jobString>Making heart.</jobString>
    <workAmount>500</workAmount>
    <workSpeedStat>TailoringSpeed</workSpeedStat>
    <effectWorking>ButcherFlesh</effectWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Silver</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>GlitterworldMedicine</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
    </ingredients>
    <products>
      <Heart>1</Heart>
    </products>
    <fixedIngredientFilter>
	  <thingDefs>
		<li>Silver</li>
		<li>Plasteel</li>
		<li>GlitterworldMedicine</li>
	  </thingDefs>
    </fixedIngredientFilter>
    <workSkill>Medicine</workSkill>
  </RecipeDef>
  
