  <RecipeDef>
    <defName>SimpleProstheticArm</defName>
    <label>Make Replacement Prosthetic Arm</label>
    <description>Make an organic replacement prosthetic arm.</description>
    <jobString>Making prosthetic arm.</jobString>
    <workAmount>500</workAmount>
    <workSpeedStat>TailoringSpeed</workSpeedStat>
    <effectWorking>ButcherFlesh</effectWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Silver</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>30</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Uranium</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Medicine</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <SimpleProstheticArm>1</SimpleProstheticArm>
    </products>
    <fixedIngredientFilter>
	  <thingDefs>
		<li>Silver</li>
